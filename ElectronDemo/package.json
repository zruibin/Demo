{
  "name": "electron-demo",
  "productName": "Electron Demo",
  "description": "My Electron application description",
  "keywords": [],
  "main": "src/main.js",
  "version": "1.0.0",
  "author": "ruibin.chow",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "gypfile": true,
  "browserslist": [
    "last 4 versions"
  ],
  "scripts": {
    "native": "node-gyp configure && node-gyp build && node-gyp clean",
    "move": "node release.js",

    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack serve --hot --host 0.0.0.0 --config=./webpack.config.js --mode development",

    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
    "package": "npm run build",
    "postpackage": "npm run native && npm run move && electron-packager ./build/app --out=./build --overwrite --asar",

    "hot": "gulp watch:electron"
  },
  "devDependencies": {
    "electron-debug": "3.2.0",
    "electron-devtools-installer": "^3.2.0"
  },
  "dependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "antd": "^5.4.6",
    "babel-loader": "^8.2.5",
    "bindings": "^1.5.0",
    "cmake-js": "^7.2.1",
    "concurrently": "8.0.1",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.1",
    "electron": "24.1.3",
    "electron-connect": "0.6.3",
    "electron-log": "4.4.8",
    "electron-packager": "^15.5.1",
    "electron-reloader": "1.2.3",
    "electron-updater": "5.3.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "gulp": "4.0.2",
    "gulp-watch": "5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-addon-api": "^6.1.0",
    "node-gyp": "^9.3.1",
    "postcss": "^8.4.8",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.6.0",
    "postcss-pxtorem": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3"
  }
}
